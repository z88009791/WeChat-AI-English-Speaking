  font-size: 24rpx;
  color: var(--grey);
  width: calc(100% - 320rpx);
  bottom: 20rpx;
  left: 160rpx;
}






/*.record 录音进行时跳出的录音界面*/
.record {
  padding: 80rpx;
  border-radius: 30rpx;
  display: flex;
  /* 设置容器为flex布局 */
  align-items: center;
  /* 水平居中对齐项目 */
  position: fixed;
  bottom: 200rpx;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 9999;
  box-shadow: 0rpx 100rpx 100rpx 100rpx rgba(0, 0, 0, 0.2);

}

.record .recording {
  /* 音浪单根线条样式 */
  float: left;
  width: 5rpx;
  margin-left: 15rpx;
  background-color: #ffffff;
  position: relative;
  animation: recordingAnimation 1s infinite linear;
  /* 应用名称为"recordingAnimation"的动画，持续时间为3秒，无限次数重复，线性过渡 */
}

@keyframes recordingAnimation {

  /* 音浪变化 */
  0% {
    transform: scaleY(1);
  }

  /* 初始状态，高度不变 */
  50% {
    transform: scaleY(.5);
  }

  /* 中间状态，高度缩小到原来的70% */
  100% {
    transform: scaleY(1);
  }

  /* 结束状态，高度恢复到原来大小 */
}

.record .stopwatch {
  /* 计时器样式 */
  position: absolute;
  margin-bottom: -90rpx;
  margin-right: -500rpx;

}


/*.cancelrecord 取消录音的按钮*/
.cancelrecord {
  display: flex;
  /* 设置容器为flex布局 */
  align-items: center;
  /* 水平居中对齐项目 */
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 9999;
  float: none;
}

.cancelrecord view {
  margin-right: 20rpx;
}

.cancelrecord .cuIcon-close {
  padding: 20rpx;
  border-radius: 50%;
  font-size: 50rpx;
  margin: 0 auto;

}